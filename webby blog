            <script type="text/javascript">

var aDownload = document.querySelectorAll('a.download');

function aDownloadTrack($this) {
    var et_category = 'download',
        et_object = encodeURIComponent($this.getAttribute('href')),
        et_action = 'download',
        et_tags = encodeURIComponent($this.textContent);

    if (typeof ET_Event === 'undefined' || typeof ET_Event.eventStart === 'undefined') return;
    ET_Event.eventStart(et_category,et_object,et_action,et_tags);
};

for (var i = 0; i < aDownload.length; i++) {
    var $this = aDownload[i];
    $this.addEventListener('click', aDownloadTrack($this), false);
}


var jsETrackerLinks = document.querySelectorAll('.jsETrackerLink');

function jsETrackerLink($this) {
    var et_category = $this.getAttribute('data-eta'), et_object = $this.getAttribute('etb'), et_action = $this.getAttribute('etc'), et_tags = $(this).data('etd');

    if (typeof ET_Event === 'undefined' || typeof ET_Event.eventStart === 'undefined') {
        return;
    }

    ET_Event.eventStart(et_category, et_object, et_action, et_tags);
}


for (var i = 0; i < jsETrackerLinks.length; i++) {
    var $this = jsETrackerLinks[i];
    $this.addEventListener('mousedown', jsETrackerLink($this), false);
}
